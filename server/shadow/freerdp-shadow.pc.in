includedir=${prefix}/@FREERDP_INCLUDE_DIR@
libs=-lfreerdp-shadow@FREERDP_API_VERSION@ -lfreerdp-shadow-subsystem@FREERDP_API_VERSION@

Name: FreeRDP shadow
Description: FreeRDP: A Remote Desktop Protocol Implementation
